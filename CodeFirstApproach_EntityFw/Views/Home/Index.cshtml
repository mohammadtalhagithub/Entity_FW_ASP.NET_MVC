@model IEnumerable<CodeFirstApproach_EntityFw.Models.Student>
@* IEnumerable<> because of ToList() in controller*@

@{
    ViewBag.Title = "Index";
}

<h2>Entity Framework - Code First Approach</h2>

<table>
    <tr>
        <th>ID</th>
        <th>NAME</th>
        <th>GENDER</th>
        <th>AGE</th>
    </tr>
    @{
        foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Gender</td>
                <td>@item.Age</td>
            </tr>
        }
    }
</table>